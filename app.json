{
  "expo": {
    "name": "Afrilish Riders",
    "slug": "afrilish-riders",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/61d2210a-87ca-47aa-a282-7112e62d66f5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.afrilish.rider",
      "buildNumber": "24"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Afrilish to access your camera",
          "microphonePermission": "Allow Afrilish to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "owner": "afrilish",
    "extra": {
      "eas": {
        "projectId": "6afeb6f3-694b-4879-b719-0a673fc6977b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
